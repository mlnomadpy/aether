{
  "model": {
    "name": "minigpt-yat-xl",
    "architecture": "yat",
    "maxlen": 2048,
    "vocab_size": 50257,
    "embed_dim": 1536,
    "num_heads": 24,
    "feed_forward_dim": 6144,
    "num_transformer_blocks": 36,
    "dropout_rate": 0.1
  },
  "training": {
    "batch_size": 8,
    "learning_rate": 0.001,
    "max_tokens_to_process": 3000000000,
    "eval_interval": 10000,
    "eval_steps": 200,
    "val_set_size": 5000,
    "checkpoint_interval": 20000,
    "optimizer": "adamw",
    "mixed_precision": "bfloat16"
  },
  "data": {
    "dataset_name": "HuggingFaceFW/fineweb",
    "split": "train",
    "streaming": true,
    "tokenizer_name": "gpt2"
  },
  "logging": {
    "wandb_project": "aether-yat-xl-training",
    "checkpoint_dir": "./checkpoints/yat_xl",
    "log_level": "INFO",
    "log_file": null
  }
}